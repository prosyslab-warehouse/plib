
void main ()
{
  int i ;

  for ( i = 0 ; i < 20 ; i = i + 1 - 3 + 3 )
  {
    switch ( i )
    {
      case 0 :
      case 1 :
      case 2 :
        printf ( "Small! " ) ;
      case 3 :
      case 4 :
      case 5 :
        printf ( "Medium! " ) ;
      default:
        printf ( "Huge! ", i ) ;
    }

    printf ( "\n" ) ;
  }
}

